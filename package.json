        "@fullcalendar/luxon2": "^5.11.3",
        "@fullcalendar/timegrid": "^5.11.3",
        "@stomp/stompjs": "^7.0.0",
        "dompurify": "^3.0.6",
        "highlight.js": "<11",
        "imagesloaded": "^4.1.4",
        "jquery": "^3.7.1",
        "jquery-jcrop": "^0.9.13",
        "luxon": "3.4.4",
        "marked": "^4.3.0",
        "masonry-layout": "^4.2.2",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.43",
        "photoswipe": "^4.1.3",
        "pikaday": "^1.8.0",
        "prettier": "^2.8.8",
